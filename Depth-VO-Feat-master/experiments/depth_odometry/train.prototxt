# Net: DepthVO-0.0

# ----------------------------------------------------------------------
# Data input
# ----------------------------------------------------------------------

layer {
  name: "imR1"
  type: "ImageData"
  top: "imR1"
  top: "nil_R1"
  transform_param {
    mean_value: 104
    mean_value: 117
    mean_value: 123
  }
  image_data_param {
    source: "./data/dataset/kitti_eigen/train_right_1.txt"
    new_width: 608 
    new_height: 160 
    batch_size: 10
  }
  include {phase: TRAIN}
}

layer {
 name: "imL2"
 type: "ImageData"
 top: "imL2"
 top: "nil_L2"
 transform_param {
   mean_value: 104
   mean_value: 117
   mean_value: 123
 }
 image_data_param {
   source: "./data/dataset/kitti_eigen/train_left_2.txt"
   new_width: 608
   new_height: 160 
   batch_size: 10
 }
 include {phase: TRAIN}
}

layer {
 name: "imR2"
 type: "ImageData"
 top: "imR2"
 top: "nil_R2"
 transform_param {
   mean_value: 104
   mean_value: 117
   mean_value: 123
 }
 image_data_param {
   source: "./data/dataset/kitti_eigen/train_right_2.txt"
   new_width: 608
   new_height: 160 
   batch_size: 10
 }
 include {phase: TRAIN}
}

layer{
 name: "raw_K"
 type: "Data"
 top: "raw_K"
  data_param {
    source: "./data/dataset/kitti_eigen/train_K/"
    batch_size: 10
    backend: LMDB
  }
  include {phase: TRAIN}
}


layer{
 name: "T_R2L"
 type: "Data"
 top: "T_R2L"
  data_param {
    source: "./data/dataset/kitti_eigen/train_T_R2L/"
    batch_size: 10
    backend: LMDB
  }
  include {phase: TRAIN}
}

# ----------------------------------------------------------------------
# Preprocessing data
# ----------------------------------------------------------------------

layer{
  name:"imR"
  type:"Concat"
  bottom:"imR2"
  bottom:"imR1"
  top: "imR"
  concat_param{axis :1}
  include {phase: TRAIN}
}

layer{
  name:"Kx2"
  type:"Concat"
  bottom:"raw_K"
  bottom:"raw_K"
  top: "K"
  concat_param{axis :0}
  include {phase: TRAIN}
}


layer {
  name: "norm_imL2"
  type: "Scale"
  bottom: "imL2"
  top: "norm_imL2"
  param {
    lr_mult: 0
  }
  scale_param {
    filler {
      type: "constant"
      value: 0.004
    }
  }
  include {phase: TRAIN}
}

layer {
  name: "norm_imR1"
  type: "Scale"
  bottom: "imR1"
  top: "norm_imR1"
  param {
    lr_mult: 0
  }
  scale_param {
    filler {
      type: "constant"
      value: 0.004
    }
  }
  include {phase: TRAIN}
}
