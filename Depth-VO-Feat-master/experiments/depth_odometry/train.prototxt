    decay_mult: 1
  }
  param { 
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param { 
    num_output: 16
    kernel_size: 7
    stride: 2
    pad: 3
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu_0_pose"
  type: "ReLU"
  bottom: "conv_0_pose"
  top: "conv_0_pose"
}


layer {
  name: "conv_1_pose"
  type: "Convolution"
  bottom: "conv_0_pose"
  top: "conv_1_pose"
  param { 
    lr_mult: 1
    decay_mult: 1
  }
  param { 
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param { 
    num_output: 32
    kernel_size: 5
    stride: 2
    pad: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu_1_pose"
  type: "ReLU"
  bottom: "conv_1_pose"
  top: "conv_1_pose"
}


layer {
  name: "conv_2_pose"
  type: "Convolution"
  bottom: "conv_1_pose"
  top: "conv_2_pose"
  param { 
    lr_mult: 1
    decay_mult: 1
  }
  param { 
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param { 
    num_output: 64
    kernel_size: 3
    stride: 2
    pad: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu_2_pose"
  type: "ReLU"
  bottom: "conv_2_pose"
  top: "conv_2_pose"
}

layer {
  name: "conv_3_pose"
  type: "Convolution"
  bottom: "conv_2_pose"
  top: "conv_3_pose"
  param { 
    lr_mult: 1
    decay_mult: 1
  }
  param { 
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param { 
    num_output: 128
    kernel_size: 3
    stride: 2
    pad: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu_3_pose"
  type: "ReLU"
  bottom: "conv_3_pose"
  top: "conv_3_pose"
}

layer {
  name: "conv_4_pose"
  type: "Convolution"
  bottom: "conv_3_pose"
  top: "conv_4_pose"
  param { 
    lr_mult: 1
    decay_mult: 1
  }
  param { 
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param { 
    num_output: 256
    kernel_size: 3
    stride: 2
    pad: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu_4_pose"
  type: "ReLU"
  bottom: "conv_4_pose"
  top: "conv_4_pose"
}

layer {
  name: "conv_5_pose"
  type: "Convolution"
  bottom: "conv_4_pose"
  top: "conv_5_pose"
  param { 
    lr_mult: 1
    decay_mult: 1
  }
  param { 
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param { 
    num_output: 256
    kernel_size: 3
    stride: 2
    pad: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu_5_pose"
  type: "ReLU"
  bottom: "conv_5_pose"
  top: "conv_5_pose"
}

layer {
  name: "fc_0_pose"
  type: "InnerProduct"
  bottom: "conv_5_pose"
  top: "fc_0_pose"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu_fc_0_pose"
  type: "ReLU"
  bottom: "fc_0_pose"
  top: "fc_0_pose"
}

layer {
  name: "fc_1_pose"
  type: "InnerProduct"
  bottom: "fc_0_pose"
  top: "fc_1_pose"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu_fc_1_pose"
  type: "ReLU"
  bottom: "fc_1_pose"
  top: "fc_1_pose"
}


layer {
  name: "temporal_pose_0"
  type: "InnerProduct"
  bottom: "fc_1_pose"
  top: "temporal_pose_0"
  param { lr_mult: 0.1 decay_mult: 1 }
  param { lr_mult: 0.2 decay_mult: 0 }
  inner_product_param {
    num_output: 6
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}


layer {
  name: "temporal_pose"
  type: "Reshape"
  bottom: "temporal_pose_0"
  top: "T_2to1" # transformation from t2 to t1 
  reshape_param {
    shape {
      dim: 0  # copy the dimension from below
      dim: 0
      dim: 1
      dim: 1 # infer it from the other dimensions
    }
  }
}


# ----------------------------------------------------------------------
# Geometry modules
# ----------------------------------------------------------------------
layer{
  name:"T"
  type:"Concat"
  bottom:"T_R2L"
  bottom:"T_2to1"
  top: "T"
  concat_param{axis :0}
  include {phase: TRAIN}
}

layer {
  name: "SE3"
  type: "Python"
  bottom: "T" 
  top: "SE3"
  python_param { 
    module: "pygeometry"
    layer: "SE3_Generator_KITTI"
  }
  include {phase: TRAIN}
}


layer{
  name:"inv_depth_imR2x2"
  type:"Concat"
  bottom:"inv_depth_imR2"
  bottom:"inv_depth_imR2"
  top: "inv_depth"
  concat_param{axis :0}
  include {phase: TRAIN}
}

layer {
  name: "invDepth2Depth"
  type: "Power"
  bottom: "inv_depth"
  top: "depth"
  include {
    phase: TRAIN
  }
  power_param {
    power: -1
    scale: 1
    shift: 0.0001
  }
}

layer {
  name: 'geoTransform'
  type: 'GeoTransform'
  bottom: "depth"
  bottom: "SE3" 
  bottom: "K"
  top: "pts3D"
  include {
    phase: TRAIN
  }
}

layer {
  name: 'pin_hole'
  type: 'PinHole'
  bottom: "pts3D"
  bottom: "K"
  top: "flows"
  top: "proj_coords"
  include {
    phase: TRAIN
  }
}


layer{
  name:"Isrc"
  type:"Concat"
  bottom:"norm_imL2"
  bottom:"norm_imR1"
  top: "Isrc"
  concat_param{axis :0}
  include {phase: TRAIN}
}

layer {
  name: "warp_Itgt"
  type: "InverseWarping"
  bottom: "Isrc"
  bottom: "proj_coords"
  top: "warp_Itgt"
  include {phase: TRAIN}
}

# ----------------------------------------------------------------------
# Photometric Loss
# ----------------------------------------------------------------------
layer {
  name: "slice_warp_Itgt"
  type: "Slice"
  bottom: "warp_Itgt"
  top: "warp_Itgt_LR" 
  top: "warp_Itgt_R12" 
  slice_param { 
    axis: 0
    slice_point: 10 
  }
}


layer {
  name: "warp_error_LR"
  type: "AbsLoss"
  bottom: "warp_Itgt_LR"
  bottom: "norm_imR2"
  top: "Warp_error_LR"
  loss_weight: 1
  include {phase: TRAIN}
}

layer {
  name: "warp_error_R12"
  type: "AbsLoss"
  bottom: "warp_Itgt_R12"
  bottom: "norm_imR2"
  top: "Warp_error_R12"
  loss_weight: 1
  include {phase: TRAIN}
}




# ----------------------------------------------------------------------
# Smoothness regularization
# ----------------------------------------------------------------------
layer {
  name: "gx_Itgt_RGB"
  type: "Convolution"
  bottom: "norm_imR2"
  top: "gx_Itgt_RGB"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 3
    bias_term: false
    kernel_size: 3
    group: 3
    stride: 1
    weight_filler {
      type: "EdgeX"
    }
  }
  include {phase: TRAIN}
}
layer{
 name:"gx_Itgt"
 type:"AbsVal"
 bottom: "gx_Itgt_RGB"
 top:"gx_Itgt"
 include {phase: TRAIN}
}
layer {
  name: "gx"
  type: "Convolution"
  bottom: "gx_Itgt"
  top: "gx"
   param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 1
    bias_term: false
    kernel_size: 1
    weight_filler {
      type: 'constant'
      value: -0.33
    }
  }
  include {phase: TRAIN}
}
layer{
 name:"mgx_exp"
 type:"Exp"
 bottom: "gx"
 top:"gx"
 include {phase: TRAIN}
}

layer {
  name: "gy_Itgt_RGB"
  type: "Convolution"
  bottom: "norm_imR2"
  top: "gy_Itgt_RGB"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 3
    bias_term: false
    kernel_size: 3
    group: 3
    stride: 1
    weight_filler {
      type: "EdgeY"
    }
  }
  include {phase: TRAIN}
}
layer{
 name:"gy_Itgt"
 type:"AbsVal"
 bottom: "gy_Itgt_RGB"
 top:"gy_Itgt"
 include {phase: TRAIN}
}
layer {
  name: "gy"
  type: "Convolution"
  bottom: "gy_Itgt"
  top: "gy"
   param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 1
    bias_term: false
    kernel_size: 1
    weight_filler {
      type: 'constant'
      value: -0.33
    }
  }
  include {phase: TRAIN}
}
layer{
 name:"mgy_exp"
 type:"Exp"
 bottom: "gy"
 top:"gy"
 include {phase: TRAIN}
}


layer {
  name: "gx_inv_depth_imR2"
  type: "Convolution"
  bottom: "inv_depth_imR2"
  top: "gx_inv_depth_imR2"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 1
    bias_term: false
    kernel_size: 3
    group: 1
    stride: 1
    weight_filler {
      type: "EdgeX"
    }
  }
  include {phase: TRAIN}
}


layer {
  name: "gy_inv_depth_imR2"
  type: "Convolution"
  bottom: "inv_depth_imR2"
  top: "gy_inv_depth_imR2"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 1
    bias_term: false
    kernel_size: 3
    group: 1
    stride: 1
    weight_filler {
      type: "EdgeY"
    }
  }
  include {phase: TRAIN}
}


layer {
  name: "edge_aware_gx_inv_depth_imR2"
  type: "Eltwise" 
  bottom: "gx"
  bottom: "gx_inv_depth_imR2"
  top: "dx"
  eltwise_param { operation: PROD }
  include {phase: TRAIN}
}

layer {
  name: "edge_aware_gy_inv_depth_imR2"
  type: "Eltwise"
  bottom: "gy"
  bottom: "gy_inv_depth_imR2"
  top: "dy"
  eltwise_param { operation: PROD }
  include {phase: TRAIN}
}


layer {
  name: "zeros"
  type: "Scale"
  bottom: "dx"
  top: "zeros_grad"
  param {
    lr_mult: 0
  }
  scale_param {
    filler {
      type: "constant"
      value: 0
    }
  }
  include {phase: TRAIN}
}

layer {
  name: "smoothness1"
  type: "AbsLoss"
  bottom: "zeros_grad"
  bottom: "dx"
  top: "dx_abs"
  loss_weight: 10
  include {phase: TRAIN}
}

layer {
  name: "smoothness2"
  type: "AbsLoss"
  bottom: "zeros_grad"
  bottom: "dy"
  top: "dy_abs"
  loss_weight: 10
  include {phase: TRAIN}
}




layer {
  name: "silence_train"
  type: "Silence"
  bottom: "nil_L2"
  bottom: "nil_R1"
  bottom: "nil_R2"
  bottom: "flows"
  include {phase: TRAIN}
}

