      type: "constant"
      value: 2
    }
  }
}
layer {
  name: "scale_pool1"
  type: "Scale"
  bottom: "conv_1"
  top: "scale_pool1"
  param {
    lr_mult: 0.01
  }
  scale_param {
    filler {
      type: "constant"
      value: 0.000001
    }
  }
}
layer {
  name: "score_pool1"
  type: "Convolution"
  bottom: "scale_pool1"
  top: "score_pool1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 1 
    pad: 0
    kernel_size: 1
  }
}

layer {
  name: "fuse_pool1"
  type: "Eltwise"
  bottom: "upscore_pool2"
  bottom: "score_pool1"
  top: "fuse_pool1"
  eltwise_param {
    operation: SUM
  }
}
layer {
  name: "upscore_all"
  type: "Deconvolution"
  bottom: "fuse_pool1"
  top: "upscore_all"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 1 
    bias_term: false
    kernel_size: 4
    group: 1
    stride: 2
    pad:1
    weight_filler {
      type: "bilinear"
    }
  }
}

layer {
  name: "inv_depth_imR"
  type: "Scale"
  bottom: "upscore_all"
  top: "inv_depth_imR_0" 
  param {
    lr_mult: 0
  }
  scale_param {
    filler {
      type: "constant"
      value: 0.01
    }
  }
}

layer {
 name: "relu_inv_depth_imR2"
 type: "ReLU"
 bottom: "inv_depth_imR_0"
 top: "inv_depth_imR2"
}

# ----------------------------------------------------------------------
