    new_height: 160 
    batch_size: 15
  }
  include {phase: TRAIN}
}

layer{
 name: "K"
 type: "Data"
 top: "K"
  data_param {
    source: "./data/dataset/kitti_eigen/train_K/"
    batch_size: 15
    backend: LMDB
