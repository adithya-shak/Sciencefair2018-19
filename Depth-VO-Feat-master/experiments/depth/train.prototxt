layer {
  name: "imL1"
  type: "ImageData"
  top: "imL"
  top: "nil_L1"
  transform_param {
    mean_value: 104
    mean_value: 117
    mean_value: 123
  }
  image_data_param {
    source: "./data/dataset/kitti_eigen/train_left_1.txt"
    new_width: 608 
    new_height: 160 
    batch_size: 15
  }
  include {phase: TRAIN}
}
layer {
  name: "imR1"
  type: "ImageData"
  top: "imR"
  top: "nil_R1"
