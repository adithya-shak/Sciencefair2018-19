  name: "inv_depth"
  type: "Scale"
  bottom: "upscore_all"
  top: "inv_depth" 
  param {
    lr_mult: 0
  }
  scale_param {
    filler {
      type: "constant"
      value: 0.01
    }
  }
}

layer {
 name: "relu_inv_depth"
 type: "ReLU"
 bottom: "inv_depth"
 top: "inv_depth"
}

layer {
  name: "invDepth2Depth"
  type: "Power"
  bottom: "inv_depth"
  top: "depth"
  include {
    phase: TRAIN
  }
  power_param {
    power: -1
    scale: 1
    shift: 0.0001
  }
}

# ----------------------------------------------------------------------
# Geometry modules
# ----------------------------------------------------------------------
layer {
  name: "SE3"
  type: "Python"
  bottom: "T_R2L" 
  top: "SE3"
  python_param { 
    module: "pygeometry"
    layer: "SE3_Generator_KITTI"
  }
  include {phase: TRAIN}
}

layer {
  name: 'geoTransform'
  type: 'GeoTransform'
  bottom: "depth"
  bottom: "SE3" 
  bottom: "K"
  top: "pts3D"
  include {
    phase: TRAIN
  }
}

layer {
  name: 'pin_hole'
  type: 'PinHole'
  bottom: "pts3D"
  bottom: "K"
  # Pin Hole layer outputs both optical flows and projected coordinates. ONLY proj_coords is used in here.
  top: "flows" 
  top: "proj_coords"
  include {
    phase: TRAIN
  }
}


layer {
  name: "warp_Itgt"
  type: "InverseWarping"
  bottom: "Isrc"
  bottom: "proj_coords"
  top: "warp_Itgt"
  include {phase: TRAIN}
}

# ----------------------------------------------------------------------
# Photometric Loss
# ----------------------------------------------------------------------
layer {
  name: "warp_error"
  type: "AbsLoss"
  bottom: "warp_Itgt"
  bottom: "Itgt"
  top: "Warp_error"
  loss_weight: 1
  include {phase: TRAIN}
}


# ----------------------------------------------------------------------
# Smoothness regularization
# ----------------------------------------------------------------------
layer {
  name: "gx_Itgt_RGB"
  type: "Convolution"
  bottom: "Itgt"
  top: "gx_Itgt_RGB"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 3
    bias_term: false
    kernel_size: 3
    group: 3
    stride: 1
    weight_filler {
      type: "EdgeX"
    }
  }
  include {phase: TRAIN}
}
layer{
 name:"gx_Itgt"
 type:"AbsVal"
 bottom: "gx_Itgt_RGB"
 top:"gx_Itgt"
 include {phase: TRAIN}
}
layer {
  name: "gx"
  type: "Convolution"
  bottom: "gx_Itgt"
  top: "gx"
   param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 1
    bias_term: false
    kernel_size: 1
    weight_filler {
      type: 'constant'
      value: -0.33
    }
  }
  include {phase: TRAIN}
}
layer{
 name:"mgx_exp"
 type:"Exp"
 bottom: "gx"
 top:"gx"
 include {phase: TRAIN}
}

layer {
  name: "gy_Itgt_RGB"
  type: "Convolution"
  bottom: "Itgt"
  top: "gy_Itgt_RGB"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 3
    bias_term: false
    kernel_size: 3
    group: 3
    stride: 1
    weight_filler {
      type: "EdgeY"
    }
  }
  include {phase: TRAIN}
}
layer{
 name:"gy_Itgt"
 type:"AbsVal"
 bottom: "gy_Itgt_RGB"
 top:"gy_Itgt"
 include {phase: TRAIN}
}
layer {
  name: "gy"
  type: "Convolution"
  bottom: "gy_Itgt"
  top: "gy"
   param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 1
    bias_term: false
    kernel_size: 1
    weight_filler {
      type: 'constant'
      value: -0.33
    }
  }
  include {phase: TRAIN}
}
layer{
 name:"mgy_exp"
 type:"Exp"
 bottom: "gy"
 top:"gy"
 include {phase: TRAIN}
}


layer {
  name: "gx_inv_depth"
  type: "Convolution"
  bottom: "inv_depth"
  top: "gx_inv_depth"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 1
    bias_term: false
    kernel_size: 3
    group: 1
    stride: 1
    weight_filler {
      type: "EdgeX"
    }
  }
  include {phase: TRAIN}
}


layer {
  name: "gy_inv_depth"
  type: "Convolution"
  bottom: "inv_depth"
  top: "gy_inv_depth"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 1
    bias_term: false
    kernel_size: 3
    group: 1
    stride: 1
    weight_filler {
      type: "EdgeY"
    }
  }
  include {phase: TRAIN}
}


layer {
  name: "edge_aware_gx_inv_depth"
  type: "Eltwise" 
  bottom: "gx"
  bottom: "gx_inv_depth"
  top: "dx"
  eltwise_param { operation: PROD }
  include {phase: TRAIN}
}

layer {
  name: "edge_aware_gy_inv_depth"
  type: "Eltwise"
  bottom: "gy"
  bottom: "gy_inv_depth"
  top: "dy"
  eltwise_param { operation: PROD }
  include {phase: TRAIN}
}


layer {
  name: "zeros"
  type: "Scale"
  bottom: "dx"
  top: "zeros_grad"
  param {
    lr_mult: 0
  }
  scale_param {
    filler {
      type: "constant"
      value: 0
    }
  }
  include {phase: TRAIN}
}

layer {
  name: "smoothness1"
  type: "AbsLoss"
  bottom: "zeros_grad"
  bottom: "dx"
